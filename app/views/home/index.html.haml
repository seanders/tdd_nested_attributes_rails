%h1 Home#index
= form_for @user do |f|
  = f.label :first_name
  = f.text_field :first_name
  = f.label :last_name
  = f.text_field :last_name
  = f.label :email
  = f.email_field :email
  = f.fields_for :involvements do |involvement_field|
    =involvement_field.text_field :start_date
    =involvement_field.text_field :end_date
  = f.submit "Create User"
